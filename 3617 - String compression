#include <iostream>
#include <string.h>
using namespace std;
int arr[1000005];
char cadena[1000005];
int m;
void kmpPreprocess(){
    int i=0,j=-1;
    arr[0] = -1;
    while(i < m){
        while(j >= 0 && cadena[i] != cadena[j]) j = arr[j];
        i++;
        j++;
        arr[i] = j;
    }
}

int solve(){
    int maxi=-1;
    for(int x=1;x<=m;x++){
        if(arr[x] == 0 && x>maxi) maxi = x;
    }
    if(arr[m]%maxi) return m;
    return maxi;
}
int main(){
    ios_base::sync_with_stdio(false); cin.tie(0);
    cin >> cadena;
    m = strlen(cadena);
    kmpPreprocess();
    int res = solve();
    cout << ((res == m) ? 0 : res )<< "\n";
}
